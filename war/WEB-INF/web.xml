<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>Timelines.html</welcome-file>
  </welcome-file-list>
  <servlet>
      <servlet-name>TimelineDataServiceImpl</servlet-name>
      <servlet-class>edu.mki.timelines.server.TimeLineDataServiceImpl</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>TimelineDataServiceImpl</servlet-name>
      <url-pattern>/timelines/timelines</url-pattern>
  </servlet-mapping>

</web-app>
